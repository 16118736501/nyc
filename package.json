{
  "name": "nyc",
  "version": "2.0.5",
  "description": "a code coverage tool that works well with subprocesses.",
  "main": "index.js",
  "scripts": {
    "test": "./bin/nyc.js tap ./test/nyc-test.js",
    "coverage": "./bin/nyc.js report --reporter=text-lcov | coveralls"
  },
  "bin": {
    "nyc": "./bin/nyc.js"
  },
  "config": {
    "nyc": {
      "exclude": [
        "node_modules/"
      ]
    }
  },
  "keywords": [
    "coverage",
    "subprocess",
    "testing",
    "reporter"
  ],
  "contributors": [
    "Isaac Schlueter <i@izs.me>"
  ],
  "author": "Ben Coe <ben@npmjs.com>",
  "license": "ISC",
  "dependencies": {
    "foreground-child": "^1.1.0",
    "istanbul": "^0.3.14",
    "jsonstream": "^1.0.3",
    "lodash": "^3.8.0",
    "mkdirp": "^0.5.0",
    "rimraf": "^2.3.3",
    "signal-exit": "^2.0.0",
    "spawn-wrap": "^0.1.2",
    "strip-bom": "^1.0.0",
    "yargs": "^3.8.0"
  },
  "devDependencies": {
    "chai": "^2.3.0",
    "coveralls": "^2.11.2",
    "standard": "^3.7.3",
    "tap": "^1.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bcoe/nyc.git"
  }
}
